[alias]
	br = branch
	ci = commit
	co = checkout
	d = diff
	dc = diff --cached
	f = ls-files
	l = log
	ll = log --graph
	lll = log --graph --all
	st = status
[color]
	ui = auto
[user]
	name = Takumi Abe
	email = abe@engraphia.com
[log]
	date = short
[format]
	pretty = %C(yellow)%h%Cblue%d%Creset %s %C(green bold)%an, %ad(%ar)%Creset
[push]
	default = current
